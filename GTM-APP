<script>

var form = document.querySelector('ng-untouched ng-pristine ng-invalid');

form.addEventListener('submit', function (e) {

var fmail = form.querySelector (“.auth-wrapper-form form”).value;


var form_data = {
email: fmail,
}

userengage('event.register_success', form_data)
 
UE.pageHit({ 
 email: fmail,
 });
 
 dataLayer.push(form_data);
 dataLayer.push({
 'event': 'register_success',
 'formType': 'Sign Up',
 'formPosition': 'Aoolication'
 });
});
</script>
