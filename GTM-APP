<script>

var form = document.querySelector(“.auth-wrapper-form form”);

form.addEventListener('submit', function (e) {

var fmail = orm.querySelector(‘#mat-input-0’).value;


var form_data = {
email: fmail,
}

userengage('event.register_success', form_data)
 
UE.pageHit({ 
 email: fmail,
 });
 
 dataLayer.push(form_data);
 dataLayer.push({
 'event': 'register_success',
 'formType': 'Sign Up',
 'formPosition': 'Aoolication'
 });
});
</script>
